{
  "name": "Archivist",
  "dockerComposeFile": [
    "docker-compose.yml",
    "docker-compose.extend.yml"
  ],
  "remoteUser": "vscode",
  "service": "archivist",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "settings": {
        "editor.formatOnSave": true,
        "editor.rulers": [
          98
        ],
        "git.inputValidation": true,
        "terminal.integrated.defaultProfile.linux": "zsh"
      },
      "extensions": [
        "eamodio.gitlens",
        "JakeBecker.elixir-ls",
        "ms-azuretools.vscode-docker"
      ]
    }
  }
}